{
  "name": "corbado/client-php",
  "version": "1.0.2",
  "description": "This client allows you to easy access API from Corbado GmbH",
  "homepage": "https://www.corbado.com",
  "authors": [
    {
      "name": "Tobias Albrecht",
      "email": "tobias.albrecht@corbado.com"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {"Corbado\\": "src/"}
  },
  "require": {
    "guzzlehttp/guzzle": "^7.4",
    "psr/cache": "^3.0",
    "symfony/cache": "^6.2",
    "firebase/php-jwt": "^6.4",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "analyze": "vendor/bin/phpstan --memory-limit=1000000000 --level=8 analyze src examples tests",
    "test": "vendor/bin/phpunit --colors=always --bootstrap vendor/autoload.php tests"
  }
}
