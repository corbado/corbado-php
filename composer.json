{
  "name": "corbado/php-sdk",
  "description": "This PHP SDK eases the integration of Corbado's passkey-first authentication solution.",
  "version": "1.0.4",
  "homepage": "https://www.corbado.com",
  "authors": [
    {
      "name": "Tobias Albrecht",
      "email": "tobias.albrecht@corbado.com"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {"Corbado\\": "src/"}
  },
  "require": {
    "guzzlehttp/guzzle": "^7.5",
    "psr/cache": "^3.0",
    "firebase/php-jwt": "^6.4",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "analyze": "vendor/bin/phpstan --memory-limit=1000000000 analyze src examples tests",
    "test": "vendor/bin/phpunit --colors=always --bootstrap vendor/autoload.php tests"
  }
}
